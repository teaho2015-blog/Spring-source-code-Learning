{
  "title": "Spring Learning|teaho.net",
  "plugins": ["disqus",
    "include-codeblock",
    "ga",
    "anchors",
    "plantuml",
    "back-to-top-button",
    "page-toc-button",
    "git-author",
    "-lunr", "-search", "search-plus"
  ],
  "pluginsConfig": {
    "disqus": {
      "shortName": "spring-source-code-learning-gitbook-teaho-net"
    },
    "include-codeblock": {
      "template": "default",
      "unindent": true,
      "theme": "monokai"
    },
    "ga": {
      "token": "UA-90704189-1"
    },
    "page-treeview": {
      "copyright": "",
      "minHeaderCount": "4",
      "minHeaderDeep": "10"

    },
    "git-author":{
      "position": "bottom",
      "createTpl": "Created by {user} {timeStamp}",
      "modifyTpl": "",
      "timeStampFormat": "YYYY-MM-DD HH:mm:ss"
    }
  }
}





